
default:
	make -C common
	make -C cim-provider
	make -C snmp-agent

all:
	make -C common all
	make -C cim-provider all
	make -C snmp-agent all

rebuild:
	make -C common rebuild
	make -C cim-provider rebuild
	make -C snmp-agent rebuild

clean:
	make -C common clean
	make -C cim-provider clean
	make -C snmp-agent clean
