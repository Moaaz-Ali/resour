
default:
	make -C common
	make -C cim-provider
	make -C snmp-agent
	make -C daemon

all:
	make -C common all
	make -C cim-provider all
	make -C snmp-agent all
	make -C daemon all

rebuild:
	make -C common rebuild
	make -C cim-provider rebuild
	make -C snmp-agent rebuild
	make -C daemon rebuild

clean:
	make -C common clean
	make -C cim-provider clean
	make -C snmp-agent clean
	make -C daemon clean
